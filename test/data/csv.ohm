// This grammar is used by the CSV example in `docs/index.md`. Do not change
// this without copying the change to index.md.

    CSV {
      csv = line*
      line = ~end (~eol fields)* eol
      fields = ListOf<field, ",">
      field = (~(eol | ",") _)*
      eol = "\r"? "\n" -- char
          | end        -- endOfFile
    }
